<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="618.0" prefWidth="1000.0" stylesheets="@../customer.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.view.customerui.customerui.AppealPageUIController">
   <children>
      <VBox alignment="CENTER" layoutX="85.0" layoutY="59.0" prefHeight="542.0" prefWidth="860.0">
         <children>
            <TextArea fx:id="appealArea" prefHeight="465.0" prefWidth="860.0" promptText="//填写申诉理由" />
            <HBox alignment="CENTER" prefHeight="43.0" prefWidth="542.0">
               <children>
                  <Button fx:id="submitButton" mnemonicParsing="false" onAction="#submit" prefHeight="27.0" prefWidth="155.0" text="提交" textFill="WHITE">
                     <styleClass>
                        <String fx:value="transButton" />
                        <String fx:value="backButtonColor" />
                     </styleClass></Button>
               </children>
            </HBox>
         </children>
      </VBox>
      <Button fx:id="backButton" layoutX="21.0" layoutY="22.0" mnemonicParsing="false" onAction="#back" prefHeight="38.0" prefWidth="91.0">
         <graphic>
            <ImageView fitHeight="31.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../CustomerImage/返回-2.png" />
               </image>
            </ImageView>
         </graphic>
         <styleClass>
            <String fx:value="transButton" />
            <String fx:value="backButtonColor" />
         </styleClass></Button>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="474.0" layoutY="20.0" prefHeight="33.0" prefWidth="77.0" text="申诉">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
