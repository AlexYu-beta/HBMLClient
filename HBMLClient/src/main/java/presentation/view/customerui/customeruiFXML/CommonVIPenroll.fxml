<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="618.0" prefWidth="1000.0" stylesheets="@../customer.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.view.customerui.customerui.EnrollVIPPageController">
   <children>
      <Button fx:id="backButton" layoutX="21.0" layoutY="22.0" mnemonicParsing="false" onAction="#back" prefHeight="38.0" prefWidth="91.0">
         <graphic>
            <ImageView fitHeight="31.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../CustomerImage/返回-2.png" />
               </image>
            </ImageView>
         </graphic>
         <styleClass>
            <String fx:value="transButton" />
            <String fx:value="backButtonColor" />
         </styleClass>
      </Button>
      <VBox alignment="CENTER" layoutX="237.0" layoutY="58.0" prefHeight="440.0" prefWidth="703.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="80.0" prefWidth="200.0">
               <padding>
                  <Insets left="24.0" />
               </padding>
               <children>
                  <ComboBox fx:id="typeComboBox" prefWidth="150.0" promptText="普通会员" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="80.0" prefWidth="378.0" spacing="50.0">
               <children>
                  <Label alignment="CENTER" prefWidth="100.0" text="姓名">
                     <styleClass>
                        <String fx:value="titleLabel" />
                        <String fx:value="backButtonColor" />
                     </styleClass>
                  </Label>
                  <TextField fx:id="nameField" prefHeight="35.0" prefWidth="503.0" />
               </children>
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </HBox>
            <HBox fx:id="birthHBox" alignment="CENTER" prefHeight="80.0" prefWidth="378.0" spacing="50.0">
               <children>
                  <Label alignment="CENTER" prefWidth="100.0" text="生日">
                     <styleClass>
                        <String fx:value="titleLabel" />
                        <String fx:value="backButtonColor" />
                     </styleClass>
                  </Label>
                  <DatePicker fx:id="birthPicker" prefHeight="32.0" prefWidth="503.0" />
               </children>
            </HBox>
            <HBox fx:id="contactHBox" alignment="CENTER" prefHeight="80.0" prefWidth="378.0" spacing="50.0">
               <children>
                  <Label alignment="CENTER" prefWidth="100.0" text="联系方式">
                     <styleClass>
                        <String fx:value="titleLabel" />
                        <String fx:value="backButtonColor" />
                     </styleClass>
                  </Label>
                  <TextField fx:id="numberField" prefHeight="35.0" prefWidth="503.0" promptText="移动电话" />
               </children>
            </HBox>
            <HBox fx:id="companyNameHBox" alignment="CENTER" prefHeight="80.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefWidth="100.0" text="企业名称" textAlignment="CENTER">
                     <styleClass>
                        <String fx:value="titleLabel" />
                        <String fx:value="backButtonColor" />
                     </styleClass>
                  </Label>
                  <TextField fx:id="companyNameField" prefHeight="35.0" prefWidth="503.0" promptText="合作企业名称" />
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox alignment="CENTER" layoutX="18.0" layoutY="142.0" prefHeight="229.0" prefWidth="219.0" spacing="10.0">
         <children>
            <ImageView fx:id="headImageView" fitHeight="125.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../CustomerImage/个人信息-2.png" />
               </image>
            </ImageView>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <Button fx:id="enrollButton" layoutX="444.0" layoutY="508.0" mnemonicParsing="false" onAction="#enroll" prefHeight="37.0" prefWidth="114.0" text="注册" textFill="WHITE">
         <styleClass>
            <String fx:value="transButton" />
            <String fx:value="backButtonColor" />
         </styleClass>
      </Button>
   </children>
</AnchorPane>
