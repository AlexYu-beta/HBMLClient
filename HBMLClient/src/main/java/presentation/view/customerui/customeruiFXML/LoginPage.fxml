<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="294.0" prefWidth="502.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.view.customerui.customerui.LoginPageUIController">
   <children>
      <VBox alignment="BOTTOM_CENTER" layoutX="1.0" prefHeight="309.0" prefWidth="500.0">
         <children>
            <HBox alignment="CENTER" prefHeight="41.0" prefWidth="500.0">
               <children>
                  <Label alignment="CENTER" prefHeight="16.0" prefWidth="71.0" text="登录" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="53.0" prefWidth="500.0" spacing="30.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefWidth="40.0" text="用户名" />
                  <TextField fx:id="nameField" prefHeight="32.0" prefWidth="373.0" promptText="用户名" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="53.0" prefWidth="500.0" spacing="30.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefWidth="40.0" text="密码" />
                  <PasswordField fx:id="passwordField" prefHeight="32.0" prefWidth="372.0" promptText="密码" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="51.0" prefWidth="500.0" spacing="30.0">
               <children>
                  <Button fx:id="loginButton" mnemonicParsing="false" onAction="#login" prefHeight="42.0" prefWidth="130.0">
                     <graphic>
                        <ImageView fitHeight="41.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../CustomerImage/成功.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
                  <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancel" prefWidth="127.0">
                     <graphic>
                        <ImageView fitHeight="41.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../CustomerImage/取消-2.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="51.0" prefWidth="500.0" spacing="30.0">
               <children>
                  <Button fx:id="enrollButton" mnemonicParsing="false" onAction="#enroll" prefHeight="42.0" prefWidth="130.0">
                     <graphic>
                        <ImageView fitHeight="41.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../CustomerImage/注册.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
                  <Button fx:id="findPasswordButton" mnemonicParsing="false" onAction="#findPassword" prefHeight="42.0" prefWidth="130.0">
                     <graphic>
                        <ImageView fitHeight="41.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../CustomerImage/密码找回.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
               </children>
               <padding>
                  <Insets top="3.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
